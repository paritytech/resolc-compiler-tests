pragma solidity >=0.4.16;

contract Test {
    function main(uint8 param) public pure returns(uint64) {
        if (param < 3) {
            if (param < 2) {
                if (param == 0) {
                    return 4;
                } else {
                    return 5 * param; // 5
                }
            }
            return param * 3; // 6
        } else {
            if (param == 3) {
                return param * 2 + 1; // 7
            }
            return 99;
        }
    }
}