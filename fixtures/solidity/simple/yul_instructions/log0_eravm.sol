pragma solidity >=0.4.16;

contract Test {
    function main(uint256 offset, uint256 len) external {
        assembly {
            // nullify memory ptr slot
            mstore(0x40, 0)
            log0(offset, len)
        }
    }

    function with_value(uint256 offset, uint256 len, uint8 val) external {
        assembly {
            for { let i := 0 } lt(i, len) { i := add(i, 1) }
            {
                mstore8(add(offset, i), val)
            }
            log0(offset, len)
            // TODO: can be removed after the VM fix
            return(0, 0)
        }
    }
}
