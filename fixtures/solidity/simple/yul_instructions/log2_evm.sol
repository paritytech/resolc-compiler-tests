// SPDX-License-Identifier: MIT

pragma solidity >=0.4.16;

contract Test {
    function main(
        uint256 offset,
        uint256 len,
        uint256 t1,
        uint256 t2
    ) external {
        assembly {
            // nullify memory ptr slot
            mstore(0x40, 0)
            log2(offset, len, t1, t2)
        }
    }

    function with_value(
        uint256 offset,
        uint256 len,
        uint8 val,
        uint256 t1,
        uint256 t2
    ) external {
        assembly {
            for {
                let i := 0
            } lt(i, len) {
                i := add(i, 1)
            } {
                mstore8(add(offset, i), val)
            }
            log2(offset, len, t1, t2)
            // TODO: can be removed after the VM fix
            return(0, 0)
        }
    }
}
